[core]
	excludesfile = ~/.gitignore_global
[user]
	name = Andre Abreu
	email = asabreu@yahoo.com
[commit]
	template = ~/.stCommitMsg
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
